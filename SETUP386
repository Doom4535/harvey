echo MAKE SURE PLAN9 is set
objtype=386
export objtype
cputype=386/unix
export cputype
NXM=`pwd`
export NXM
#PATH=$PATH:~/src/plan9port/bin/
PATH=$NXM/xbin:$PLAN9/bin:$PATH:$PLAN9/bin:.
# Linux is so smart sometimes, but we'll be using P9P ls which doesn't understand
alias ls=ls
#!/bin/bash

set -x
set -e

PATH=$PATH:$PLAN9/bin
export NXM=`pwd`
. $NXM/buildconfig
(cd util/8a && sh BUILD)
(cd util/8l && sh BUILD)
(cd util/8c && sh BUILD)
(cd util && sh BUILD)
(cd inferno-os && sh BUILD)
