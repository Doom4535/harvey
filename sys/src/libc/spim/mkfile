objtype=spim
</$objtype/mkfile

LIB=/$objtype/lib/libc.a
SFILES=\
	main9.s\
	main9p.s\
	argv0.s\
	getfcr.s\
#	memchr.s\
#	memmove.s\
#	memset.s\
	setjmp.s\
#	strchr.s\
#	strcpy.s\

CFILES=\
	notejmp.c\
	sqrt.c\

HFILES=/sys/include/libc.h

OFILES=${CFILES:%.c=%.$O} ${SFILES:%.s=%.$O}

UPDATE=mkfile\
	$HFILES\
	$CFILES\
	$SFILES\

</sys/src/cmd/mksyslib
